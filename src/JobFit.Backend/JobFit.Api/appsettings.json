{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CacheSettings": {
    "AbsoluteExpirationInSeconds": 1200,
    "SlidingExpirationInSeconds": 300
  },
  "CorsSettings": {
    "AllowAnyOrigins": true,
    "AllowCredentials": false,
    "AllowAnyMethods": true,
    "AllowAnyHeaders": true
  }
}
